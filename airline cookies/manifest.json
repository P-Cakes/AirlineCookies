
{
  "manifest_version": 2,
  "name": "AirlineCookieBreaker",
  "description": "An extension which searches random flights to mess with cookies.",
  "version":"1.0",
  "author": "Patrick S",
  "permissions":["tabs","activeTab"],

  "background": {
    "scripts":["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }

}